draw_text_colour((vw - string_width(title)) / 2, ycenter - (ds_list_size(menu) + 2) * offset, title, tcolor, tcolor, c_dkgray, c_dkgray, 1);
// menu
for (var i = 0; i &lt; ds_list_size(menu); i++)
{
    var item = menu[| i],
        color = ternary(i == selection, c_white, c_ltgray),
        ix = (vw - string_width(item)) / 2,
        iy = ycenter - (ds_list_size(menu) - 1) * offset;
    draw_text_colour(ix, iy + i * offset, item, color, color, color, color, 1);
    // selection
    if (i == selection)
        draw_text_colour(ix - string_width(indicator) - 4, iy + i * offset, indicator, color, color, color, color, 1);
}
draw_set_font(-1);
</string>
