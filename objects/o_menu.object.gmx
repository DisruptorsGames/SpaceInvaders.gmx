var offset = 24;
if (!is_undefined(menu[| 0]))
    offset = font_get_size(f_title)/*string_height(menu[| 0])*/;

var ycenter = vh / 2;

// title
draw_set_font(f_title);
draw_text_colour((vw - string_width(title)) / 2, ycenter - (ds_list_size(menu) + 2) * offset, title, c_black, c_black, c_black, c_black, 1);
